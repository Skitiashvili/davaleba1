7.1
let array = [ 2, 3, 5, 10, 25, 24, 11, 100, 6, 7, 10 ]

for (let i = 0; i < array.length; i++) {
    if (array[i]%2 != 1) {
    console.log( "ლუწი რიცხვები", array[i])
    }
}

7.2
let array = [ 2, 3, 5, 10, 25, 24, 11, 100, 6, 7, 10 ]

for (let i = 0; i < array.length; i++) {
    if (array[i]%2 != 0) {
    console.log( "კენტი რიცხვები", array[i])
    }
}
